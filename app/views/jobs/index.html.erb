All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul class="jobs">
    <li><%= job.title %> at <%= job.company.name %></li>
   <li><%= link_to "Edit", edit_company_job_path(@company, job) %> </li>
   <li><%= link_to "Delete", company_job_path(@company, job), method: :delete %> </li>
 </ul>
<% end %>

<p><%= link_to "Add a Contact", new_company_contact_path(@company) %></p>

  <% @company.contacts.each do |contact| %>
    <ul class="contact">
      <p><%= contact.full_name %> <%= contact.position %><%= contact.email %></p>
    <% end %>
  </ul>
