<%= form_for [@company, @job] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :level_of_interest %>
  <%= f.number_field :level_of_interest %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :category %>
  <%= f.select :category, Category.all.pluck(:title) %>
  <%= link_to "Create a New Category", new_category_path %> 

  <%= f.submit %>
<% end %>
